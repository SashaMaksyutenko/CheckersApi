<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<PlatformTarget>x64</PlatformTarget>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Swashbuckle.AspNetCore" Version="10.1.0" />
		<PackageReference Include="Swashbuckle.AspNetCore.SwaggerUI" Version="10.1.0" />
	</ItemGroup>

	<!-- Автоматичне копіювання Kingsrow64.dll у bin\Debug\net9.0\ -->
	<ItemGroup>
		<None Update="Kingsrow64.dll">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="egdb64.dll">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="Kingsrow.odb">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="weights_v4.bin">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="weights_nk_v4.bin">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>

</Project>